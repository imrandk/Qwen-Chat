async function fetchPrompts(){try{const t=await fetch("https://raw.githubusercontent.com/imrandk/Prompt-Lit/main/Prompt%20List");if(!t.ok)throw new Error;return(await t.text()).split("\n").filter((t=>t.trim()))}catch{return[]}}async function setRandomPrompt(){const t=document.querySelector("textarea#chat-input");if(!t)return;const e=await fetchPrompts();e.length&&(t.value=e[Math.floor(Math.random()*e.length)].replace(/^Prompt:\s*/,""),t.dispatchEvent(new Event("input",{bubbles:!0})),setTimeout((()=>document.querySelector("button#send-message-button")?.click()),1e3),setTimeout(findAndClickOtherButton,2e3))}function findAndClickOtherButton(){const t=setInterval((()=>{const e=document.querySelector(".flex-raw > .flex:nth-child(1) > .visible");e&&(e.click(),clearInterval(t),setTimeout(setRandomPrompt,1e3))}),1e3)}setInterval((()=>console.clear()),1e4),setRandomPrompt();
